    var no = Convert.ToInt32(ViewData["TableNo"]);

    for (int i = 1; i < 11; i++)
    {
        <h1> @no*@i=@(no * @i)   </h1>
    }


    <h1> TempData Table No: @TempData["TableNo"]</h1>
    TempData.Keep();
}


